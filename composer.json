{
    "name": "xiranst/media-bundle",
    "type": "symfony-bundle",
    "language": "PHP",
    "description": "Help your application to upload file easier.",
    "keywords": [ "upload image", "upload file", "upload media", "imagine", "image", "manipulation", "pictures", "photos", "transformation", "bundle", "symfony", "xiranst" ],
    "homepage": "http://www.xiranst.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Xiranst Inc. and other contributors",
            "homepage": "https://github.com/xiranst/XiranstMediaBundle/graphs/contributors"
        }
    ],
    "require": {
        "php":             ">=5.4.0",
        "symfony/symfony": ">=3.0"
    },
    "autoload": {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4": {
            "Xiranst\\Bundle\\MediaBundle\\": ""
        }
    },
    "minimum-stability":"dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}